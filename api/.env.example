# OpenAI API Key (for GPT models)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (for Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Azure OpenAI (optional)
AZURE_API_KEY=your_azure_api_key_here
AZURE_API_BASE=your_azure_endpoint_here
AZURE_API_VERSION=2024-02-15-preview

# Google AI (optional)
GOOGLE_API_KEY=your_google_api_key_here

# Cohere (optional)
COHERE_API_KEY=your_cohere_api_key_here

# CB Insights API (for startup research)
CB_INSIGHTS_API_KEY=your_cb_insights_api_key_here

# Google Maps API (for directions)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Database URL (optional, defaults to SQLite)
DATABASE_URL=sqlite:///./startup_swiper.db

# JWT Secret for authentication
SECRET_KEY=your_secret_key_here_change_in_production

# Web Push Notifications (VAPID)
# Generate these with: python3 generate_vapid_keys.py
VAPID_PUBLIC_KEY=your_vapid_public_key_here
VAPID_PRIVATE_KEY=your_vapid_private_key_here

# Logging
LOG_LEVEL=INFO
